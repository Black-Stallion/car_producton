  <!-- 批量删除 -->
	<delete id="deleteBatch">
		DELETE
		FROM
			c_keepapply
		WHERE
			apply_id IN
		<foreach collection="array" item="id" open="(" close=")" separator=",">
			#{id}
		</foreach>
	</delete>
	<select id="searchCarKeepApproveByCarKeepApproveDriverfrom" parameterType="string" resultMap="BaseResultMap">
		SELECT
			  *
		FROM 
			c_keepapply
		WHERE
		0==0
			<if test="departmentId!=null">
				and department_id=#{departmentId}
			</if>
		AND
			approve LIKE CONCAT( '%',#{approve},'%' ) 
	</select>

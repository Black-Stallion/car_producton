<%@ page language="java" contentType="text/html; charset=UTF-8" pageEncoding="UTF-8"%>
<link href="js/kindeditor-4.1.10/themes/default/default.css" type="text/css" rel="stylesheet">

<link href="css/uploadfile.css" rel="stylesheet"> 
<script src="js/jquery.uploadfile.js"></script>
<script src="js/malsup.github.iojquery.form.js"></script>

<script type="text/javascript" charset="utf-8" src="js/kindeditor-4.1.10/kindeditor-all-min.js"></script>
<script type="text/javascript" charset="utf-8" src="js/kindeditor-4.1.10/lang/zh_CN.js"></script>
<div style="padding:10px 10px 10px 10px">
	<form id="carRemoveEditForm" class="carRemoveForm" method="post">
		<input type="hidden" name="removeId"/>
	    <center>
	    <table cellpadding="5">
	     <tr>
	            <td>购入车辆:</td>
	            <td>
	            	<input class="easyui-combobox" id="carrrsCombox" data-options="required:true,editable:false" panelHeight="auto" size="9px" />
	            	<input name="rbeiyong1" id="rbeiyong1" type="hidden"/>
    				<input class="easyui-combobox" id="carrrNumbersCombox" data-options="required:true,editable:false" panelHeight="auto" size="10px"/>  
    				<input name="newnum" id="newnum" type="hidden"/>
    				</td>
	        </tr>
	        <tr>
	            <td>购入人:</td>
	            <td>
	            	<input  class="easyui-combobox" name="rbieyong2" panelHeight="auto"
	            	data-options="valueField:'username',textField:'username',
    						url:'user/find_data', editable:false, required:true" />
	            </td>
	        </tr>
	        <tr>
	            <td>接收单位:</td>
	             <td>
	            	<input class="easyui-combobox" id="departmenComboxz1" data-options="required:true" panelHeight="auto" size="9px" />
	            	<input name="departmentFrist" id="departmentFrist" type="hidden"/>
    				<input class="easyui-combobox" id="departmentz1"  panelHeight="auto" size="10px"/>  
    				<input name="inorgan" id=inorgan type="hidden"/>
	            </td>
	        </tr>
	       
	        <tr>
	            <td>调拨文号:</td>
	            <td>
	            	<input class="easyui-textbox" type="text" name="removenum" style="width: 200px;"></input>
	            </td>
	        </tr>
	        <tr>
	            <td>购入日期:</td>
	            <td>
	            	<input class="easyui-datetimebox" name="removetime"     
        			data-options="required:true,showSeconds:true" value="date.format('yyyy-MM-dd hh:mm:ss')" style="width:150px"></input>
	            </td>
	        </tr>
	         <tr>
	            <td>类型:</td>
	            <td>
	            	<input class="easyui-textbox" type="text" name="stats" data-options="required:true,editable:false" value="购入"></input>
	            </td>
	        </tr>
	    </table>
	     </center>
	</form>
	<div style="padding:5px 0 0 55px">
	    <a href="javascript:void(0)" class="easyui-linkbutton" onclick="submitCarRemoveEditForm()">提交</a>
	</div>
</div>
<script type="text/javascript">
	
	function submitCarRemoveEditForm(){
		if(!$('#carRemoveEditForm').form('validate')){
			$.messager.alert('提示','表单还未填写完成!');
			return ;
		}
		
		$.post("carRemove/update_all",$("#carRemoveEditForm").serialize(), function(data){
			if(data.status == 200){
				$.messager.alert('提示','修改信息成功!','info',function(){
					$("#carRemoveEditWindow").window('close');
					$("#carRemoveList").datagrid("reload");
				});
			}else{
				$.messager.alert('提示', data.msg);
			}
		});
	}
	$('#departmenComboxz1').combobox({  
        url: "department/get_data_first",  
        editable:false,  
        valueField:'departmentName',   
        textField:'departmentName',  
        panelHeight:'auto',  
        onSelect : function(data){  
            $('#departmentFrist').val(data.departmentId);  
            $('#inorgan').val(data.departmentName);  
            var fatherdep= $('#departmentFrist').val();
          
            $('#departmentz1').combobox({  
               
                url:"department/get_data_second/"+fatherdep,  
                valueField:'departmentName',  
                textField:'departmentName' ,
                editable:false,
                onSelect : function(data1){  
                  $('#inorgan').val(data1.departmentName);  
                }
           
            }).combobox('clear');  
        }  
    });  
	$('#carrrsCombox').combobox({  
        url: "carPublic/get_data",  
        editable:false,  
        valueField:'usenature',   
        textField:'usenature',  
        panelHeight:'auto',  
        onSelect : function(data){  
            $('#rbeiyong1').val(data.usenature);  
            var fatherdep= $('#rbeiyong1').val();
          
            $('#carrrNumbersCombox').combobox({  
               
                url:"carPublic/find_nature/"+fatherdep,  
                valueField:'carnumber',  
                textField:'carnumber' ,
                panelHeight:'auto',  
                editable:false,
                onSelect : function(data1){  
                  $('#newnum').val(data1.carnumber);  
                }
           
            }).combobox('clear');  
        }  
    });  
</script>

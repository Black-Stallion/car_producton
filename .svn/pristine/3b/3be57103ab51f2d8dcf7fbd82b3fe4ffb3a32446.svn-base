  <select id="find" parameterType="string" resultMap="BaseResultMap">
		SELECT
				*
		FROM 
			c_keepapply
		WHERE 
			0 = 0
			and stats='未审批'
			
			and approve=#{approve}
			 order by startdate
	</select>
	
	<select id="find1" parameterType="carApprove" resultMap="BaseResultMap">
		SELECT
			*
		FROM 
			c_keepapply
		WHERE 
			0 = 0
		<if test="departmentId!=null">
			and department_id=#{departmentId}
		</if>
			and stats='未审批'
			 order by startdate
	</select>
	
	<select id="getXX" parameterType="carRegister" resultMap="BaseResultMap">
		SELECT
			*
		FROM 
			c_keepapply
			order by startdate
	</select>
	<select id="findByDepartment" parameterType="string" resultMap="BaseResultMap">
